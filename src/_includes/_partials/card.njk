<a href="{{ post.url | url }}" class="card postlist-link">
  <div class="card__copy">
    <h3>
      {% if post.data.title %}
        {{ post.data.title }}
      {% else %}
        <code>{{ post.url }}</code>
      {% endif %}
    </h3>

    {% if post.data.subtitle %}
      <h4>
        {{ post.data.subtitle }}
      </h4>
    {% endif %}
  </div>
</a>